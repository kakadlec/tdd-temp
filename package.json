  "main": "server/server.js",
  "scripts": {
    "start:dev": "nodemon -r dotenv/config server/server.js",
    "test": "node -r dotenv/config ./node_modules/jest/bin/jest.js --env node --watch --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
