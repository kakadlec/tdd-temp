    "helmet": "^4.4.1",
    "knex": "^0.21.17",
    "knex-stringcase": "^1.4.5",
    "mssql": "^6.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "dotenv": "^8.2.0",
